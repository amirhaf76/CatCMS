@using TinyMCE.Blazor

<Editor ScriptSrc=".\lib\tinymce\tinymce.js"
        LicenseKey="gpl"
        JsConfSrc="sample"
        Value=@Content
        ValueChanged=@ContentChanged/>


@code {
    [Parameter]
    public string? Content { get; set; }
    [Parameter] 
    public EventCallback<string?> ContentChanged { get; set; }
}
